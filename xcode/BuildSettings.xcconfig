ARCHS = $(ARCHS_STANDARD_32_64_BIT)
SDKROOT = macosx
MACOSX_DEPLOYMENT_TARGET = 10.6

CLANG_CXX_LIBRARY = libstdc++
CLANG_CXX_LANGUAGE_STANDARD = c++98

SHARED_WARNING_CFLAGS = -Weverything -Wno-float-equal -Wno-c++98-compat-pedantic -Wno-weak-vtables -Wno-padded -Wno-documentation
WARNING_CFLAGS = $(SHARED_WARNING_CFLAGS)

EXPORTED_SYMBOLS_FILE = exports.exp
